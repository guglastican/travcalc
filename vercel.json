{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/calculate-distance",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/index.html",
      "dest": "server.js"
    },
    {
      "src": "/distance.html",
      "dest": "server.js"
    },
    {
      "src": "/calculator.html",
      "dest": "server.js"
    }
  ]
}
