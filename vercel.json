{
  "version": 2,
  "redirects": [
    {
      "source": "/how-far-london-to-edinburgh(/?)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/how-far-london-to-paris(/?)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/how-far-london-to-stonehenge(/?)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/how-far-london-to-new-york(/?)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/how-far-london-to-cambridge(/?)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/how-far-london-to-scotland(/?)",
      "destination": "/",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "blog/_site/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.xml",
      "use": "@vercel/static"
    },
    {
      "src": "*.xsl",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sitemap-dynamic.xml",
      "dest": "server.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "server.js"
    },
    {
      "src": "/distance/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/places/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/turnaround/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/sitemap-main.xml",
      "dest": "/sitemap-main.xml"
    },
    {
      "src": "/sitemap.xsl",
      "dest": "/sitemap.xsl"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/terms.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/turnaround-time-calculator",
      "dest": "/turnaround-time-calculator.html"
    },
    {
      "src": "/places",
      "dest": "/places.html"
    },
    {
      "src": "/distance",
      "dest": "/distance.html"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/([^/.]+)",
      "dest": "/$1.html"
    }
  ]
}